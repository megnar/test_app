pipeline {
    agent none
    stages {
        stage('run app') {
            agent {
                label 'built-in'
            }
            steps {
                sh 'pip3 install -r requirements.txt'
                sh 'python3 main.py'
            }
        }
        stage('Hello') {
            agent {
                label 'Local'
            }
            steps {
                echo 'Hello World'
            }
        } 
    }
}

pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('download requirements') {
            steps {
                sh 'pip3 install -r requirements.txt'
            }
        }
        stage('test'){
           steps {
               sh 'python3 main.py'
           }
        }
    }
}
